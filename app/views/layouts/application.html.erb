<html>
  <head>
    <title>Flatiron Store</title>
  </head>
  <body>
    <% if current_user %>
      <%= link_to current_user.name, current_user %> |
      <%= link_to 'Sign Out', signout_path, method: :delete %>
    <% else %>
      <%= link_to "Sign Up", new_user_path %> |
      <%= link_to "Sign In", signin_path %>
    <% end %></br>
    </br>
    <%= yield %>

  </body>
</html>
